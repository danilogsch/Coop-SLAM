          </material>
          </visual-->
        <collision name='collision'>
          <geometry>
            <sphere>
              <radius>0.145</radius>
            </sphere>
          </geometry>
        </collision>
      </link>
      
      <link name='caster_front'>
        <pose>0.357138 -0 -0.075 0 -0 0</pose>
        <inertial>
          <mass>1</mass>
          <inertia>
            <ixx>0.1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1</iyy>
            <iyz>0</iyz>
            <izz>0.1</izz>
