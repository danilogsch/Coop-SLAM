

      <model name='human_vehicle'>
      <pose>0 0 0.325 0 0 0</pose>

      <link name='base_footprint'>
        <pose>0 -0 0.1 0 -0 0</pose>
        <inertial>
          <mass>1.14395</mass>
          <inertia>
            <ixx>0.126164</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.416519</iyy>
            <iyz>0</iyz>
            <izz>0.481014</izz>
          </inertia>
        </inertial>
        <!--visual name='visual'>
          <geometry>
          <cylinder>
          <radius>0.55</radius>
          <length>0.250</length>
          </cylinder>
          </geometry>
          <material>
            <ambient>0.5 0.5 1.0 1</ambient>
            <diffuse>0.5 0.5 1.0 1</diffuse>
            <specular>0.0 0.0 1.0 1</specular>
          </material>
        </visual-->
        <collision name='collision'>
          <geometry>
            <cylinder>
          <radius>0.55</radius>
          <length>0.150</length>
          </cylinder>
          </geometry>
        </collision>
        <sensor name="depth_camera1" type="depth_camera">
        <pose>0 0 4 0 1.570796 0</pose>
          <topic>human_vehicle/depth_camera</topic>
          <camera>
            <horizontal_fov>1.57</horizontal_fov>
            <image>
              <width>800</width>
              <height>600</height>
              <format>R_FLOAT32</format>
            </image>
            <clip>
              <near>0.1</near>
              <far>10.0</far>
            </clip>
            <save enabled="false">
              <path>depth_data</path>
            </save>
          </camera>
          <always_on>1</always_on>
          <update_rate>10</update_rate>
          <visualize>true</visualize>
          </sensor>
          <sensor name="instance_segmentation_camera" type="segmentation">
          <pose>0 0 4 0 1.570796 0</pose>
          <topic>human_vehicle/panoptic</topic>
          <camera>
            <segmentation_type>instance</segmentation_type>
            <horizontal_fov>1.57</horizontal_fov>
            <image>
              <width>800</width>
              <height>600</height>
            </image>
            <clip>
              <near>0.1</near>
              <far>10</far>
            </clip>
            <save enabled="true">
              <path>segmentation_data/instance_camera</path>
            </save>
          </camera>
          <always_on>1</always_on>
          <update_rate>10</update_rate>
          <visualize>true</visualize>
        </sensor>
      </link>

      <link name='left_wheel'>
        <pose>0.0 0.3 -0.075 -1.570707963267948966 0 0</pose>
        <inertial>
          <mass>2</mass>
          <inertia>
            <ixx>0.145833</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.145833</iyy>
            <iyz>0</iyz>
            <izz>0.125</izz>
          </inertia>
        </inertial>
        <!--visual name='visual'>
