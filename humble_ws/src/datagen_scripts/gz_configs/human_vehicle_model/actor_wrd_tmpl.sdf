          </geometry>
        </collision>
      </link>

      <link name='right_wheel'>
        <pose>0.0 -0.3 -0.075 -1.570707963267948966 0 0</pose>
        <inertial>
          <mass>2</mass>
          <inertia>
            <ixx>0.145833</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.145833</iyy>
            <iyz>0</iyz>
